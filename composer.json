{
    "name": "osu-wams/pharm_drupal",
    "description": "Drupal Project for Oregon State University - College of Pharmacy",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "composer/installers": "^1.0",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/address": "^2.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/advagg": "^6.0@alpha",
        "drupal/autocomplete_deluxe": "^2.0.1",
        "drupal/automated_crop": "^3.0",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/block_class": "^2.0",
        "drupal/bootstrap_layout_builder": "^2.2",
        "drupal/bootstrap_styles": "^1.1",
        "drupal/bpmn_io": "^2.0",
        "drupal/calendar_view": "^2.1",
        "drupal/ckeditor5_font": "1.x-dev@dev",
        "drupal/ckeditor5_paste_filter": "^1.0",
        "drupal/ckeditor_bs_grid": "^2.0",
        "drupal/ckeditor_font": "^2.0@beta",
        "drupal/cl_server": "^2.0@beta",
        "drupal/coffee": "^1.4",
        "drupal/computed_field": "^4.0@beta",
        "drupal/config_filter": "^2.4",
        "drupal/config_ignore": "^3.2",
        "drupal/config_pages": "^2.15",
        "drupal/config_readonly": "^1.0@beta",
        "drupal/config_split": "^2.0@RC",
        "drupal/config_update": "^2.0@alpha",
        "drupal/cookies": "^1.2",
        "drupal/core": "^10.2",
        "drupal/core-composer-scaffold": "^10.2",
        "drupal/custom_field": "^3.0@RC",
        "drupal/custom_view_filters": "^2.0",
        "drupal/devel": "^5.0",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/eca": "^2.0",
        "drupal/editor_advanced_link": "^2.2",
        "drupal/entity_clone": "^2.0@beta",
        "drupal/fast_404": "^3.3",
        "drupal/featureflags": "^2.0",
        "drupal/field_group": "^3.4",
        "drupal/geofield_map": "^3.0",
        "drupal/gin": "^3.0@RC",
        "drupal/gin_lb": "^1.0@RC",
        "drupal/gin_login": "^2.0",
        "drupal/gin_toolbar": "^1.0@RC",
        "drupal/gtranslate": "^3.0",
        "drupal/honeypot": "^2.1",
        "drupal/image_widget_crop": "^2.4",
        "drupal/imageapi_optimize": "^4.0",
        "drupal/imagick": "^1.11",
        "drupal/inline_block_title_automatic": "^1.1",
        "drupal/inline_entity_form": "^2.0@RC",
        "drupal/instagram_feeds": "2.0.x-dev@dev",
        "drupal/layout_builder_browser": "^1.5",
        "drupal/layout_builder_component_attributes": "^2.1",
        "drupal/layout_builder_iframe_modal": "^1.3",
        "drupal/layout_builder_instant_preview": "^1.0",
        "drupal/layout_builder_operation_link": "^2.1",
        "drupal/layout_builder_restrictions": "^2.18",
        "drupal/layout_builder_save_and_edit": "^1.0",
        "drupal/mailchimp": "^2.2",
        "drupal/masquerade": "^2.0@beta",
        "drupal/media_entity_instagram": "^3.0",
        "drupal/media_library_edit": "^3.0",
        "drupal/media_thumbnails": "^1.0@beta",
        "drupal/menu_block": "^1.11",
        "drupal/menu_breadcrumb": "^2.0@alpha",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^2.0",
        "drupal/module_filter": "^4.0",
        "drupal/nomarkup": "^1.0",
        "drupal/path_redirect_import": "^2.0",
        "drupal/pathauto": "^1.11",
        "drupal/prlp": "^1.10",
        "drupal/recaptcha": "^3.2",
        "drupal/redirect": "^1.9",
        "drupal/revision_log_default": "^1.0",
        "drupal/sdc_display": "^1.0@beta",
        "drupal/sdc_styleguide": "^1.0@beta",
        "drupal/seckit": "^2.0",
        "drupal/section_library": "^1.1",
        "drupal/simple_menu_icons": "^3.0",
        "drupal/simple_sitemap": "^4.1",
        "drupal/smart_date": "^4.1@RC",
        "drupal/smart_trim": "^2.1",
        "drupal/smtp": "^1.2",
        "drupal/spamspan": "^3.1",
        "drupal/stage_file_proxy": "^3.1",
        "drupal/storage": "^1.4@beta",
        "drupal/storybook": "^1.0@beta",
        "drupal/superfish": "^1.7",
        "drupal/tagify": "^1.0",
        "drupal/token": "^1.10",
        "drupal/tracer": "^1.0",
        "drupal/twig_tweak": "^3.1.0",
        "drupal/webform": "^6.2",
        "drupal/webp": "^1.0@beta",
        "drupal/webprofiler": "^10.1",
        "drupal/yoast_seo": "^2.0@alpha",
        "drush/drush": "^12",
        "palantirnet/drupal-rector": "^0.19.2",
        "vlucas/phpdotenv": "^4.0",
        "webflo/drupal-finder": "^1.0.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/core-dev": "^10.2",
        "drupal/sdc_examples": "^1.0@dev",
        "phpstan/phpstan": "^1.11"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "cweagans/composer-patches": true,
            "tbachert/spi": true,
            "zaporylie/composer-drupal-optimizations": true
        },
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },    
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            }
        },
        "patches": {
            "drupal/bootstrap_layout_builder": {
                "#3375928": "https://www.drupal.org/files/issues/2023-07-20/3375928-depecated_notice_when_configuring_columns-3.patch"
            },
            "drupal/ckeditor5_font": {
                "https://www.drupal.org/project/ckeditor5_font/issues/3368736 ('no config schema' Error)": "https://www.drupal.org/files/issues/2024-07-18/ckeditor5_font-schema-fix-10_3_x-3368736-53.patch"
            },
            "drupal/core": {
                "https://www.drupal.org/project/drupal/issues/3346430 (Wrong revision loaded in Entity::doPreSave)": "https://git.drupalcode.org/project/drupal/-/merge_requests/3599.diff"
            },
            "drupal/instagram_feeds": {
                "https://www.drupal.org/project/instagram_feeds/issues/3319317": "https://git.drupalcode.org/project/instagram_feeds/-/merge_requests/4.patch"
            },
            "drupal/sdc_display": {
                "https://www.drupal.org/project/sdc_display/issues/3377609 (token replacement)": "https://git.drupalcode.org/project/sdc_display/-/merge_requests/1.patch"
            },
            "drupal/menu_block": {
                "https://www.drupal.org/project/menu_block/issues/758184": "https://www.drupal.org/files/issues/2024-08-28/menu_block-active_level_fallback_when_no_children-758184-46.patch"
            },
            "drupal/simple_menu_icons": {
                "#3459278": "https://git.drupalcode.org/project/simple_menu_icons/-/merge_requests/10.diff"
            },
            "drupal/menu_breadcrumb": {
                "https://www.drupal.org/project/menu_breadcrumb/issues/3274684": "https://www.drupal.org/files/issues/2022-04-11/menu_breadcrumb_exclude_menus_items_with_query_3274684-2.patch"
            }
        }
    }
}
